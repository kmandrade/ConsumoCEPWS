@model CepViewModel

<form class="row g-3 needs-validation" novalidate>
  <div class="col-md-4">
    <label class="form-label">Endereço</label>
        <input type="text" class="form-control" value="@Model.Rua" disabled readonly>
  </div>
  <div class="col-md-4">
    <label class="form-label">Bairro</label>
        <input type="text" class="form-control" value="@Model.Bairro" disabled readonly>
    
  </div>
  <div class="col-md-4">
        <label  class="form-label">Cidade</label>
        <input type="text" class="form-control" value="@Model.Cidade" disabled readonly>
  </div>
  
  <div class="col-md-3">
    <label  class="form-label">Estado</label>
        <input type="text" class="form-control" value="@Model.Estado" disabled readonly>
    
  </div>
  
</form>